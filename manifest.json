{

  "description": "This add-on filters phishing banking site",
  "manifest_version": 2,
  "name": "Banking filter",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": ["html/alert.html"],
  "background": {
    "scripts": ["background.js", "popup.js"]
  }

}
